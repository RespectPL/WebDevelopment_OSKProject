<div class="container-fluid">
    <div class="row my-4 mx-auto text-center col-7">
        <p align="left"><b>Lista instruktorów</b></p>
        <table class="table">
            <thead class="table-primary">
                <tr>
                    <th scope="col">Imię i nazwisko</th>
                    <th scope="col">Telefon</th>
                </tr>
            </thead>
            <tbody>
                {{#each items}}
                <tr>
                    <td>{{this.imie}} {{this.nazwisko}}</td>
                    <td>{{this.telefon}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>